      <schema>
        <key>/schemas/apps/brightside/corner_delay</key>
        <applyto>/apps/brightside/corner_delay</applyto>
        <owner>brightside</owner>
        <type>int</type>
        <default>500</default>
        <locale name="C">
          <short>Corner delay</short>
          <long>Delay in milliseconds before screen corner action occurs</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/brightside/edge_delay</key>
        <applyto>/apps/brightside/edge_delay</applyto>

        <default>false</default>
        <locale name="C">
          <short>Enable edge flip</short>
          <long>Switch to the adjacent workspace when the mouse reaches the edge of the screen</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/brightside/after_flip_command</key>
